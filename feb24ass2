n=int(input("Enter number of elements: "))
for i in range(1,n+1):
    listElements=int(input("Enter telement %d: "%i))
    n.append(listElements)

evenList=[] 
oddList=[]

for j in n:
    if j%2==0:
        evenList.append(j)
    else:
        oddList.append(j)

print("Even number list: ",evenList)
print("Odd number list: ",oddList) 
